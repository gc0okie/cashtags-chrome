{
    "name": "cashtag info",
    "description": "Shows cnbc info on cashtags you hover over in Twitter",
    "version": "0.1.0",
    "manifest_version": 2,
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches": [
            "https://www.twitter.com/*",
            "https://twitter.com/*"],
        "js": ["jquery.js","main.js"],
        "run_at": "document_end"
        }
    ],
    "permissions": [
        "tabs",
        "https://www.twitter.com/*",
        "https://twitter.com/*"
    ]
}